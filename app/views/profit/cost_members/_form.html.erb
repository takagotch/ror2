<%= markup do |m|
	p = CostMemberFormPreenter.new(f,self)
	m << p.notes
	p.with_options(required: true) do |q|
		m << q.text_field_block(:password, 'Pass', size: 32)
		m << q.password_field_block(:password, 'Pass', size: 32)
		m << q.full_name_block(:family_name, :given_name, 'Name')
		m << q.full_name_block(:fimily_name_kana, :given_name_kana, 'kana')
		m << q.date_field_block(:start_date, 'start date')
		m << q.date_field_block(:end_date, 'end date', required: false)
	end
	m << p.check_boxes
end %>


