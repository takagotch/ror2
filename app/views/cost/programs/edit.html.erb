<% @title = 'Edit'%>
<h1><%= @title %></h1>

<div id="generic-form">
	<%= form_for @program_form, as: 'form',
		url: [:cost, @program_form.program ] do |f| %>
		<%= render 'form', f: f %>
		<div class="buttons">
			<%= f.submit 'Update' %>
			<%= link_to 'Cancel', :cost_programs %>
		</div>
	<% end %>
</div>

