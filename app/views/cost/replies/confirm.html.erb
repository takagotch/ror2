<% @title = 'contact reply confirm' %>
<h1><%= @title %></h1>

<div id="generic-form" class="table-wrapper">
	<%= form_for @reply, url: cost_message_reply_path(@message) do |f| %>
		<%= render 'confriming_form', f: f %>
		<div class="buttons">
		<%= f.submit 'submit' %>
		<%= f.submit 'correct', :cost_messages %>
		</div>
	<% end %>
	<%= render 'message' %>
</div>

