<%= markup do |m| 
	p = CostMemberFromPresenter.new(f, self)
	m << p.notes
	p.with_options(required: true) do |q|
		m << q.text_field_block(:email, 'E-mail', size: 32)
		m << q.full_name_block(:family_name, :given_name, 'name')
		m << q.full_name_block(:family_name_kana, :given_name_kana, 'furigana')
	end
end%>

